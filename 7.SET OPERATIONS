s={2,4,5,1,9}#empty set

print("Adding element to set")

s.add(3)

s.add(10)

print(s)

print("Clear the list")

print(s.clear())

print("copy of a set")

s1={3,5,2,1,9,0}

s=s1.copy()

print(s)

print("Diference of 2 sets:")

s2={1,2,4,3,5,6}

print(s.difference(s2))

print("Pop")

print(s2)

print("Remove")

s2.remove(3)

print(s2)

print("Union")

print(s1.union(s2))

print("update")

s1.update(s)

print(s1)
